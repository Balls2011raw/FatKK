local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local DebugButton = Instance.new("TextButton")
local ColorButton = Instance.new("TextButton")
local UpdateRateButton = Instance.new("TextButton")
local ClearGuisButton = Instance.new("TextButton")
local DebugText = Instance.new("TextLabel")

ScreenGui.Parent = game.CoreGui

Frame.Size = UDim2.new(0, 200, 0, 320)
Frame.Position = UDim2.new(1, -210, 0, 20)
Frame.BackgroundTransparency = 0.5
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.Parent = ScreenGui

DebugButton.Size = UDim2.new(0, 70, 0, 25)
DebugButton.Position = UDim2.new(0, 10, 0, 290)
DebugButton.Text = "Debug: ON"
DebugButton.Parent = Frame

ColorButton.Size = UDim2.new(0, 50, 0, 25)
ColorButton.Position = UDim2.new(0, 150, 0, 290)
ColorButton.Text = "Color"
ColorButton.Parent = Frame

UpdateRateButton.Size = UDim2.new(0, 70, 0, 25)
UpdateRateButton.Position = UDim2.new(0, 150, 0, 255)
UpdateRateButton.Text = "Update: 1s"
UpdateRateButton.Parent = Frame

ClearGuisButton.Size = UDim2.new(0, 30, 0, 30)
ClearGuisButton.Position = UDim2.new(1, -40, 0, 0)
ClearGuisButton.Text = "X"
ClearGuisButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
ClearGuisButton.Parent = Frame

DebugText.Size = UDim2.new(1, -10, 0, 200)
DebugText.Position = UDim2.new(0, 5, 0, 5)
DebugText.Text = ""
DebugText.TextColor3 = Color3.fromRGB(255, 255, 255)
DebugText.BackgroundTransparency = 1
DebugText.TextWrapped = true
DebugText.TextYAlignment = Enum.TextYAlignment.Top
DebugText.Parent = Frame

local debugEnabled = true
local textColor = Color3.fromRGB(255, 255, 255)
local updateRate = 1
local updateRateValues = {0, 0.25, 0.5, 1, 2, 5, 10}
local updateRateIndex = 4

local function updateDebugInfo()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    local position = character.HumanoidRootPart.Position
    local ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
    local memory = math.floor(game:GetService("Stats"):GetTotalMemoryUsageMb())
    local speed = math.floor(character.HumanoidRootPart.Velocity.Magnitude)
    local jumpPower = humanoid and humanoid.JumpPower or 0
    local playerCount = #game.Players:GetPlayers()
    local fps = math.floor(1 / game:GetService("RunService").RenderStepped:Wait())
    local time = os.date("%X", tick())
    local health = math.floor(humanoid.Health)
    local gameName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
    local camera = workspace.CurrentCamera
    local cameraPos = camera.CFrame.Position

    local partCount = 0
    local function countParts(parent)
        for _, obj in pairs(parent:GetChildren()) do
            if obj:IsA("BasePart") then partCount = partCount + 1
            elseif obj:IsA("Model") then countParts(obj) end
        end
    end
    countParts(workspace)

    local backpackItems = #player.Backpack:GetChildren()

    if debugEnabled then
        DebugText.Text = string.format(
            "%s\nPos: %d,%d,%d\nPing: %s\nMem: %dMB\nSpeed: %d\nJump: %d\nPlayers: %d\nFPS: %d\nTime: %s\nHealth: %d\nParts: %d\nCam Pos: %.2f,%.2f,%.2f\nBackpack: %d",
            gameName, position.X, position.Y, position.Z, ping, memory, speed, jumpPower, playerCount, fps, time, health, partCount,
            cameraPos.X, cameraPos.Y, cameraPos.Z, backpackItems
        )
        DebugText.TextColor3 = textColor
    else
        DebugText.Text = ""
    end
end

DebugButton.MouseButton1Click:Connect(function()
    debugEnabled = not debugEnabled
    DebugButton.Text = debugEnabled and "Debug: ON" or "Debug: OFF"
    updateDebugInfo()
end)

ColorButton.MouseButton1Click:Connect(function()
    local colors = {Color3.fromRGB(255, 255, 255), Color3.fromRGB(255, 0, 0), Color3.fromRGB(255, 255, 0), Color3.fromRGB(0, 255, 0), Color3.fromRGB(0, 0, 255)}
    textColor = colors[(table.find(colors, textColor) % #colors) + 1]
    ColorButton.Text = textColor == Color3.fromRGB(255, 255, 255) and "White" or
                       textColor == Color3.fromRGB(255, 0, 0) and "Red" or
                       textColor == Color3.fromRGB(255, 255, 0) and "Yellow" or
                       textColor == Color3.fromRGB(0, 255, 0) and "Green" or "Blue"
    updateDebugInfo()
end)

UpdateRateButton.MouseButton1Click:Connect(function()
    updateRateIndex = (updateRateIndex % #updateRateValues) + 1
    updateRate = updateRateValues[updateRateIndex]
    UpdateRateButton.Text = updateRate == 0 and "Update: 0s" or string.format("Update: %.2fs", updateRate)
end)

ClearGuisButton.MouseButton1Click:Connect(function()
    Frame:Destroy()
end)

local lastUpdateTime = tick()
while true do
    local currentTime = tick()
    local deltaTime = currentTime - lastUpdateTime
    if updateRate == 0 or deltaTime >= updateRate then
        updateDebugInfo()
        lastUpdateTime = currentTime
    end
    wait(0.1)
en
